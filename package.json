{
  "name": "vault-launcher",
  "version": "1.1.0",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "bump": "bun version-bump.mjs",
    "dev": "bun esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && bun esbuild.config.mjs production",
    "test": "bun tests/ide-launch.test.ts",
    "test:integration": "INTEGRATION=1 bun tests/ide-launch.integration.test.ts",
    "lint": "biome check .",
    "format": "biome format --write ."
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "esbuild": "0.25.5",
    "tslib": "2.4.0",
    "typescript": "^5.8.3",
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "latest"
  },
  "dependencies": {
    "obsidian": "latest"
  },
  "private": true
}
